                <my:widgets.ClearImage width="100" height="1px"/>
                <g:Label ui:field="lastNameLabel" styleName='{style.formLabel}'/>
                <my:widgets.ClearImage width="2" height="1px"/>
                <g:TextBox ui:field="lastNameField" width="200px"  />
                <my:widgets.ClearImage width="2" height="1px"/>
                <g:Label ui:field="lastNameFieldError" styleName="{style.formError}"/>
            </g:HorizontalPanel>

            <my:widgets.ClearImage width="2" height="5"/>

