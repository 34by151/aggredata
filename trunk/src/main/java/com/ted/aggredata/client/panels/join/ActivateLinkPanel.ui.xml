<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:my='urn:import:com.ted.aggredata.client'>
    <ui:style src="../Global.css"/>

    <ui:with field='txtRes' type='com.ted.aggredata.client.resources.lang.JoinConstants'/>
    <ui:with field='dashRes' type='com.ted.aggredata.client.resources.lang.DashboardConstants'/>

    <g:VerticalPanel styleName='{style.mainBody}' horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_TOP">
        <my:widgets.ClearImage width="1024px" height="80px"/>
        <g:HorizontalPanel horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_TOP">
            <my:widgets.ClearImage width="1px" height="640px"/>
            <g:AbsolutePanel ui:field="contentPanel"/>
        </g:HorizontalPanel>
    </g:VerticalPanel>

</ui:UiBinder>